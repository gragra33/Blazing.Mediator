{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "OpenTelemetryExample.Application.Services.OpenTelemetryActivityProcessor": "Debug"
        },
        "TelemetryDatabase": {
            "BatchSize": 20,
            "ProcessingIntervalMs": 1000,
            "MinimumLogLevel": "Debug",
            "CaptureDebugLogs": true,
            "Enabled": true
        }
    },
    "TelemetryBatching": {
        "StreamingBatchSize": 100,
        "StreamingBatchTimeoutMs": 2000,
        "RegularBatchSize": 20,
        "RegularBatchTimeoutMs": 500,
        "ProcessingIntervalMs": 500,
        "EnableDetailedLogging": true
    }
}
