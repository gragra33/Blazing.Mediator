{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "OpenTelemetryExample.Application.Services.OpenTelemetryActivityProcessor": "Information"
    }
  },
  "AllowedHosts": "*",
  "TelemetryBatching": {
    "StreamingBatchSize": 1000,
    "StreamingBatchTimeoutMs": 5000,
    "RegularBatchSize": 100,
    "RegularBatchTimeoutMs": 1000,
    "ProcessingIntervalMs": 1000,
    "EnableDetailedLogging": false
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=:memory:"
  }
}