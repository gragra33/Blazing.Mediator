{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "OpenTelemetryExample.Application.Services.OpenTelemetryActivityProcessor": "Debug"
    }
  },
  "TelemetryBatching": {
    "StreamingBatchSize": 100,
    "StreamingBatchTimeoutMs": 2000,
    "RegularBatchSize": 20,
    "RegularBatchTimeoutMs": 500,
    "ProcessingIntervalMs": 500,
    "EnableDetailedLogging": true
  }
}