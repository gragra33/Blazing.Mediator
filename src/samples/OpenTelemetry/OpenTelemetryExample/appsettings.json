{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "OpenTelemetryExample.Application.Services.OpenTelemetryActivityProcessor": "Information",
            "Blazing.Mediator": "Debug"
        },
        "MediatorLogging": {
            "Comment": "Individual logging categories can be controlled here (examples):",
            "RequestMiddleware": "Debug",
            "NotificationMiddleware": "Debug", 
            "Send": "Debug",
            "SendStream": "Debug",
            "Publish": "Debug",
            "RequestPipelineResolution": "Debug",
            "NotificationPipelineResolution": "Debug",
            "Warnings": "Warning",
            "QueryAnalyzer": "Debug",
            "CommandAnalyzer": "Debug"
        },
        "TelemetryDatabase": {
            "BatchSize": 50,
            "ProcessingIntervalMs": 2000,
            "MinimumLogLevel": "Information",
            "CaptureDebugLogs": true,
            "Enabled": true,
            "SkipPatterns": [
                "TelemetryDatabaseLoggingProvider",
                "ProcessLogBatch",
                "Enqueued log:",
                "Capturing log:",
                "Successfully saved",
                "Processing batch of",
                "Microsoft.EntityFrameworkCore.Database.Command",
                "Microsoft.EntityFrameworkCore.Infrastructure",
                "Microsoft.EntityFrameworkCore.Query",
                "Microsoft.EntityFrameworkCore.Update",
                "Microsoft.EntityFrameworkCore.Database.Transaction",
                "Microsoft.EntityFrameworkCore.ChangeTracking",
                "Microsoft.EntityFrameworkCore.Model",
                "Microsoft.EntityFrameworkCore.Model.Validation",
                "POST /api/logs/test-logging",
                "POST /api/logs/test-database-logging"
            ]
        }
    },
    "AllowedHosts": "*",
    "TelemetryBatching": {
        "StreamingBatchSize": 1000,
        "StreamingBatchTimeoutMs": 5000,
        "RegularBatchSize": 100,
        "RegularBatchTimeoutMs": 1000,
        "ProcessingIntervalMs": 1000,
        "EnableDetailedLogging": false
    },
    "ConnectionStrings": {
        "DefaultConnection": "Data Source=:memory:"
    }
}
